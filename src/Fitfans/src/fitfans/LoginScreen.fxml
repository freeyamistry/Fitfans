<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="fitfans.LoginScreenController">
   <children>
      <AnchorPane layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <TextField fx:id="username" layoutX="26.0" layoutY="44.0" promptText="Username" />
            <PasswordField fx:id="pword" layoutX="26.0" layoutY="88.0" promptText="Password" />
            <Button fx:id="loginbtn" layoutX="75.0" layoutY="124.0" mnemonicParsing="false" onAction="#handleLoginButton" text="Login" />
            <Button fx:id="signupbtn" layoutX="58.0" layoutY="161.0" mnemonicParsing="false" onAction="#handleSignupButton" text="Signup Now" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
